<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jit.bgwithasset">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:name=".MyApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".asset.AssetEasyActivity" />
        <activity android:name=".background.BackgroundActivity" />
        <activity android:name=".location.LocationActivity" />
        <activity android:name=".file.FontActivity" />
        <activity android:name=".demo.PerfectDemoActivity" />
        <activity android:name=".file.FileActivity" />
        <activity android:name=".json.JsonActivity" />
        <activity android:name=".icon.HomeIconActivity" />
        <activity android:name="._imageview.LoadImageActivity" />
        <activity android:name="._imageview.IvHalfAlphaActivity" />
        <activity
            android:name="._imageview.ColorFilterActivity"
            android:label="长按灰度效果" />
        <activity android:name="._imageview.ExchangeActivity"
            android:label="bitmap以及drawable相互转换"/>
    </application>

</manifest>